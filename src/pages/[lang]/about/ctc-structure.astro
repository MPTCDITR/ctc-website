---
import Layout from "@/layouts/Layout.astro";
import { languages } from "@/i18n/ui";
import { CentersContent } from "@/components/ourCenters/centers-sections";

export async function getStaticPaths() {
  return Object.keys(languages).map((lang) => ({
    params: { lang },
  }));
}
---

<Layout title="Our Center - Community Tech Centers">
  <CentersContent client:load />
</Layout>
