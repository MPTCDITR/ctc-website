---
import Layout from "@/layouts/Layout.astro";
import { languages } from "@/i18n/ui";
import { MissionVisionContent } from "@/components/about/mission-vision-content";

export async function getStaticPaths() {
  return Object.keys(languages).map((lang) => ({
    params: { lang },
  }));
}
---

<Layout title="BlogSpace - Welcome">
  <MissionVisionContent client:load />
</Layout>
