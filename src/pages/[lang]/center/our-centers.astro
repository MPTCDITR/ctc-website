---
import Layout from "@/layouts/Layout.astro";
import { languages } from "@/i18n/ui";
import { CentersContent } from "@/components/ourCenters/centers-sections";
import { CentersInformation } from "@/components/ourCenters/centers-information";

export async function getStaticPaths() {
  return Object.keys(languages).map((lang) => ({
    params: { lang },
  }));
}
---

<Layout title="Community Tech Center - Centers Location">
  <CentersContent client:load />
  <CentersInformation client:load />
</Layout>
