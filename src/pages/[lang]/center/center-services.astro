---
import Layout from "@/layouts/Layout.astro";
import { languages, ui } from "@/i18n/ui";
import { OperationPrinciple } from "@/components/principle-operation/our-operations";
import { getLangFromUrl } from "@/lib/i18n";
export async function getStaticPaths() {
  return Object.keys(languages).map((lang) => ({
    params: { lang },
  }));
}

const lang = getLangFromUrl(Astro.url);
const translations = ui[lang];
---

<Layout title="Community Tech Center - Our provided services">
  <OperationPrinciple translations={translations} client:load />
</Layout>
