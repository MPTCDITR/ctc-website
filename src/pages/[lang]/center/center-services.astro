---
import Layout from "@/layouts/Layout.astro";
import { languages } from "@/i18n/ui";
import { ServiceSection } from "@/components/ourCenters/service-section";
export async function getStaticPaths() {
  return Object.keys(languages).map((lang) => ({
    params: { lang },
  }));
}
---

<Layout title="Community Tech Center - Our provided services">
  <ServiceSection />
</Layout>
